{
  "name": "Sudoku-AI",
  "version": "1.0.0",
  "description": "A simple web application that can solve custom sudoku puzzle",
  "main": "index.js",
  "repository": "git@github.com:JPStrydom/Sudoku-AI.git",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
