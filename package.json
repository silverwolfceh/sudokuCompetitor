    "mocha": "5.0.1",
    "pre-commit": "1.2.2",
    "prettier": "1.7.4",
    "seedrandom": "2.4.3",
    "webpack-dev-server": "2.9.3"
  },
  "dependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "express": "4.16.2",
    "lodash": "4.17.4",
    "react": "16.0.0",
